<div class="toast position-absolute" style="right: 0; top: 57px;" data-delay="10000" role="alert" aria-live="assertive" aria-atomic="true">
	<div class="toast-header">
		<%= image_tag '/concursos_assets/mosaico_natura/logo-mosaiconatura.png', alt: 'Mosaico Natura', title: 'Mosaico Natura', class: 'img-fluid rounded mr-2' %>
		<button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
			<span aria-hidden="true">&times;</span>
		</button>
	</div>
	<div class="toast-body">
		<%= notice %>
	</div>
</div>