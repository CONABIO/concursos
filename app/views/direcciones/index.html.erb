<p id="notice"><%= notice %></p>

<h1>Direcciones</h1>

<table>
  <thead>
    <tr>
      <th>Calle</th>
      <th>Numero</th>
      <th>Interior</th>
      <th>Colonia</th>
      <th>Municipio</th>
      <th>Cp</th>
      <th>Estado</th>
      <th>Id usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @direcciones.each do |direccion| %>
      <tr>
        <td><%= direccion.calle %></td>
        <td><%= direccion.numero %></td>
        <td><%= direccion.interior %></td>
        <td><%= direccion.colonia %></td>
        <td><%= direccion.municipio %></td>
        <td><%= direccion.cp %></td>
        <td><%= direccion.estado %></td>
        <td><%= direccion.id_usuario %></td>
        <td><%= link_to 'Show', direccion %></td>
        <td><%= link_to 'Edit', edit_direccion_path(direccion) %></td>
        <td><%= link_to 'Destroy', direccion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Direccion', new_direccion_path %>
