<p id="notice"><%= notice %></p>

<h1>Medias</h1>

<table>
  <thead>
    <tr>
      <th>Original filename</th>
      <th>Filename</th>
      <th>Titulo</th>
      <th>Fecha subida</th>
      <th>Ruta</th>
      <th>Size</th>
      <th>Id usuario</th>
      <th>Id categoria</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medias.each do |media| %>
      <tr>
        <td><%= media.original_filename %></td>
        <td><%= media.filename %></td>
        <td><%= media.titulo %></td>
        <td><%= media.fecha_subida %></td>
        <td><%= media.ruta %></td>
        <td><%= media.size %></td>
        <td><%= media.id_usuario %></td>
        <td><%= media.id_categoria %></td>
        <td><%= link_to 'Show', media %></td>
        <td><%= link_to 'Edit', edit_media_path(media) %></td>
        <td><%= link_to 'Destroy', media, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Media', new_media_path %>
